    "serve": "vue-cli-service serve",
    "test": "yarn run test:unit",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2",
    "@commitlint/config-conventional": "^13.2",
