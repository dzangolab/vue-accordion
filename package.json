{
  "name": "@dzangolab/vue-accordion",
  "version": "2.0.0-rc.2",
  "description": "An accordion component for vue.js (vue3)",
  "keywords": [
    "vuejs",
    "accordion"
  ],
  "homepage": "https://github.com/dzangolab/vue-accordion",
  "bugs": "https://github.com/dzangolab/vue-accordion/issues",
  "repository": {
    "type": "git",
